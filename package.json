{
  "name": "trace.moe-api",
  "version": "1.0.0",
  "description": "API server of trace.moe",
  "main": "server.ts",
  "type": "module",
  "scripts": {
    "start": "pm2 start ecosystem.config.json",
    "watch": "pm2 start ecosystem.config.json --watch",
    "stop": "pm2 stop ecosystem.config.json",
    "restart": "pm2 restart ecosystem.config.json",
    "reload": "pm2 reload ecosystem.config.json",
    "delete": "pm2 delete ecosystem.config.json",
    "prettier": "prettier",
    "format": "prettier --write \"**/*.*\"",
    "lint": "prettier --check \"**/*.*\"",
    "test": "prettier --check \"**/*.*\"",
    "build:wasm": "asc assembly/color-layout.ts -o build/color-layout.wasm --optimize --exportRuntime"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/soruly/trace.moe-api.git"
  },
  "author": "soruly",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/soruly/trace.moe-api/issues"
  },
  "homepage": "https://github.com/soruly/trace.moe-api#readme",
  "devDependencies": {
    "@types/node": "^25.0.9",
    "assemblyscript": "^0.28.9",
    "prettier": "^3.8.0",
    "prettier-plugin-embed": "^0.5.1",
    "prettier-plugin-sql": "^0.19.2"
  },
  "dependencies": {
    "@zilliz/milvus2-sdk-node": "^2.6.9",
    "aniep": "^0.6.0",
    "cors": "^2.8.5",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.12",
    "postgres": "^3.4.8",
    "sharp": "^0.34.5",
    "sqids": "^0.3.0"
  }
}
